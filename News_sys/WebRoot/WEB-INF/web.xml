<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <display-name></display-name>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<servlet-class>com.fatcat.news_sys.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>NewsManagerServlet</servlet-name>
    <servlet-class>com.fatcat.news_sys.servlet.NewsManagerServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>MemManagerServlet</servlet-name>
    <servlet-class>com.fatcat.news_sys.servlet.MemManagerServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>MangerServlet</servlet-name>
    <servlet-class>com.fatcat.news_sys.servlet.MangerServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>NewsColumnServlet</servlet-name>
    <servlet-class>com.fatcat.news_sys.servlet.NewsColumnServlet</servlet-class>
  </servlet>




  
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>NewsManagerServlet</servlet-name>
    <url-pattern>/NewsManagerServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>MemManagerServlet</servlet-name>
    <url-pattern>/MemManagerServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>MangerServlet</servlet-name>
    <url-pattern>/MangerServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>NewsColumnServlet</servlet-name>
    <url-pattern>/NewsColumnServlet</url-pattern>
  </servlet-mapping>
  
  
</web-app>
